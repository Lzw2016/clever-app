<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="">
    <select id="aaa">
        <![CDATA[
        select * from sys_jwt_token where rownum <= 10
        ]]>
    </select>

    <update id="bbb">
        update sys_jwt_token set token = #{token} where id = #{id}
    </update>
</mapper>